            .addComponent(jPanel1, javax.swing.GroupLayout.DEFAULT_SIZE, javax.swing.GroupLayout.DEFAULT_SIZE, Short.MAX_VALUE)
        );
        layout.setVerticalGroup(
            layout.createParallelGroup(javax.swing.GroupLayout.Alignment.LEADING)
            .addComponent(jPanel1, javax.swing.GroupLayout.DEFAULT_SIZE, javax.swing.GroupLayout.DEFAULT_SIZE, Short.MAX_VALUE)
        );

        pack();
    }// </editor-fold>                        

    private void DashboardMousePressed(java.awt.event.MouseEvent evt) {                                       
        Dashboard.setBackground(ClickedColor);
        Product.setBackground(DefaultColor);
        Account.setBackground(DefaultColor);
        Calendar.setBackground(DefaultColor);
        
        MenuName.setText("Dashboard");
    }                                      

    private void ProductMousePressed(java.awt.event.MouseEvent evt) {                                     
        Dashboard.setBackground(DefaultColor);
        Product.setBackground(ClickedColor);
        Account.setBackground(DefaultColor);
        Calendar.setBackground(DefaultColor);
        MenuName.setText("Product");
    }                                    

    private void AccountMousePressed(java.awt.event.MouseEvent evt) {                                     
        Dashboard.setBackground(DefaultColor);
        Product.setBackground(DefaultColor);
        Account.setBackground(ClickedColor);
        Calendar.setBackground(DefaultColor);
        MenuName.setText("Account");
    }                                    

    private void CalendarMousePressed(java.awt.event.MouseEvent evt) {                                      
        Dashboard.setBackground(DefaultColor);
        Product.setBackground(DefaultColor);
        Account.setBackground(DefaultColor);
        Calendar.setBackground(ClickedColor);
        MenuName.setText("Calendar");
    }                                     

    
